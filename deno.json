{
  "name": "@kt-tools/css-ts",
  "version": "0.3.2",
  "license": "MIT",
  "exports": {
    ".": "./mod.ts",
    "./vite": "./vite.ts"
  },
  "types": "./mod.d.ts",
  "publish": {
    "include": [
      "README.md",
      "mod.ts",
      "mod.d.ts",
      "vite.ts",
      "src/**/*.ts",
      "dist/**/*.js",
      "dist/**/*.d.ts"
    ],
    "exclude": ["!dist/", "!dist/**"]
  },
  "tasks": {
    "build": "deno run --node-modules-dir=auto -A npm:typescript/bin/tsc -p tsconfig.json",
    "test": "deno test --node-modules-dir=auto -A vite_test.ts"
  }
}
